      %em.social-icon
        = partial('/images/icons/linkedin.svg').to_s
    %a.social{ href: 'https://github.com/rrott', target: '_blank', rel: 'nofollow noopener noreferrer', title: "rrott GitHub account" }
      %em.social-icon
        = partial('/images/icons/github.svg').to_s
  %div
    %a.social{ href: 'https://twitter.com/roman_rott', target: '_blank', rel: 'nofollow noopener noreferrer', title: "Twitter acc of Roman Rott" }
      %em.social-icon
        = partial('/images/icons/twitter.svg').to_s
  %div
    %a.social{ href: "skype:roman.rott?chat", title: "Roman Rott's Skype: roman.rott" }
      %em.social-icon
        = partial('/images/icons/skype.svg').to_s
